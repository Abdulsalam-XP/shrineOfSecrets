{
    "version": 2,
    "builds": [
      {
        "src": "api/shrineOfSecrets.js",
        "use": "@vercel/node"
      },
      {
        "src": "bot.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/api/shrineOfSecrets.js"
      },
      {
        "src": "/bot",
        "dest": "/bot.js"
      }
    ]
  }
  